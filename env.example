# Environment Variables Example
# Copy this to .env for local development

# Application
NODE_ENV=development
APP_PORT=8201

# Database (optional - only if your app needs it)
DB_HOST=mysql
DB_PORT=3306
DB_NAME=app_db
DB_USER=app_user
DB_PASSWORD=devpassword
DB_ROOT_PASSWORD=rootpassword

# Redis (optional - only if your app needs it)
REDIS_HOST=redis
REDIS_PORT=6379

# Development specific
DEBUG=true
LOG_LEVEL=debug